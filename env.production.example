# Production Environment Variables for EC2
# Copy this to .env on your EC2 instance

# Database Configuration
POSTGRES_PASSWORD=your-secure-postgres-password-here

# Security
SECRET_KEY=your-production-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=20160
REFRESH_TOKEN_EXPIRE_DAYS=30

# Cookie Configuration (for httpOnly cookies)
COOKIE_SECURE=true
COOKIE_DOMAIN=your-domain.com
COOKIE_SAMESITE=lax

# Redis
REDIS_URL=redis://redis:6379

# OpenAI API
OPENAI_API_KEY=your-openai-api-key-here

# API Configuration
API_URL=http://your-ec2-public-ip:8000

# CORS Configuration (for production, specify your domain)
CORS_ORIGINS=http://your-ec2-public-ip:3000,http://your-domain.com

# Database
DATABASE_URL=postgresql://healthup:healthup@postgres/healthup

# App Settings
ENVIRONMENT=production

# MCP Server Configuration
MCP_TIMEOUT=30

# Frontend URL
VITE_API_URL=http://your-domain.com/api 