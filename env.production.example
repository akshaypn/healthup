# Production Environment Variables for EC2
# Copy this to .env on your EC2 instance

# Database Configuration
POSTGRES_PASSWORD=your-secure-postgres-password-here

# Security
SECRET_KEY=your-super-secret-key-change-this-in-production

# Google Gemini API
GEMINI_API_KEY=your-gemini-api-key-here

# API Configuration
API_URL=http://your-ec2-public-ip:8000

# CORS Configuration (for production, specify your domain)
CORS_ORIGINS=http://your-ec2-public-ip:3000,http://your-domain.com 